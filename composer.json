{
	"name": "shazzad/wp-logs",
	"description": "Store and view logs for debugging your application.",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"require": {
		"mustache/mustache": "^2.12",
		"shazzad/github-plugin-updater": "dev-main"
	},
	"autoload": {
		"psr-4": {
			"Shazzad\\WpLogs\\": "includes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Shazzad\\WpLogs\\Commands\\": "commands"
		}
	},
	"require-dev": {
		"fakerphp/faker": "^1.24"
	}
}